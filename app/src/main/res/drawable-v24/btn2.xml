<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
                <item android:state_pressed="true" >
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#10238f" />
                                <gradient android:angle="-90" android:startColor="#132583" android:endColor="#0d175c"  />
                        </shape>
                </item>
                <item android:state_focused="true">
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#2b60af" />
                                <solid android:color="#31498f"/>
                        </shape>
                </item>
                <item >
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#415bab" />
                                <gradient android:angle="-90" android:startColor="#3864b7" android:endColor="#2e3d91" />
                        </shape>
                </item>
</selector>