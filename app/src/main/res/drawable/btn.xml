<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
                <item android:state_pressed="true" >
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#239aaf" />
                                <gradient android:angle="-90" android:startColor="#228691" android:endColor="#1e7584"  />
                        </shape>
                </item>
                <item android:state_focused="true">
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#2bafa7" />
                                <solid android:color="#318f84"/>
                        </shape>
                </item>
                <item >
                        <shape android:shape="rectangle"  >
                                <corners android:radius="20dp" />
                                <stroke android:width="1dp" android:color="#4b7eb9" />
                                <gradient android:angle="-90" android:startColor="#3d7b8b" android:endColor="#2e7091" />
                        </shape>
                </item>
</selector>